<script language="javascript">
var reserva = new Array;
var eliminados = new Array;

function conf_del(nombre,id){
 var nombre=nombre;
 var id=id;
 if (confirm("Esta ud. seguro de que de desea borrar la imagen?"))
 return window.location='?seccion=bo_catalogo&estado=2&nombre_foto='+nombre+'&id_imagen='+id ;
 else return false;
}
function crear_reserva(casilla){ 
var encontrado=0; 
pepe = reserva.length + 1 ;
pepe2 = eliminados.length + 1; 
if(casilla.defaultChecked){
 if(casilla.checked==false){
 for(i=0;i<pepe2;i++){
 if(casilla.value==eliminados[i]){
 encontrado=1; }
 }
 if(encontrado!=1){
 eliminados[eliminados.length] = casilla.value; }
 }
 }
 if(casilla.checked==false){
 for(i=0;i<pepe;i++){
 if(casilla.value==reserva[i]){
 reserva[i]="";
 }
 }
 }
 if(casilla.checked){
 for(i=0;i<pepe;i++){
 if(casilla.value==reserva[i]){
 encontrado=1; }
 }
 if(encontrado!=1){
 reserva[reserva.length] = casilla.value; }
 }
}

function mandar_reserva(enlace){
 var pasados ;
 var pasados_del ;
 pasados="";
 pasados_del="";
 for(i=0;i<reserva.length;i++){
 pasados =pasados + '&reserva['+i+']=' + reserva[i];
 }
 for(i=0;i<eliminados.length;i++){
 if(eliminados[i]){
 pasados_del =pasados_del + '&eliminados['+i+']=' + eliminados[i];
 }
 }
 pepe=enlace + pasados + pasados_del ;
 return window.location=pepe;
}
</script>
